{
    "name": "affordablemobiles/g-serverless-support-laravel",
    "description": "Google Serverless runtime support for Laravel apps",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Melrose",
            "email": "sam.melrose@affordablemobiles.co.uk"
        }
    ],
    "require": {
        "php": "^8.3",
        "a1comms/eloquent-datastore": "~1",
        "a1comms/eloquent-sqlcommenter": "~9",
        "a1comms/opencensus": "~0",
        "a1comms/opencensus-exporter-stackdriver": "~0",
        "guzzlehttp/guzzle": "^7.4.1",
        "google/cloud": ">=0.237.0 <1.0.0",
        "kelvinmo/simplejwt": "^0.6.2",
        "illuminate/support": "~9",
        "illuminate/contracts": "~9",
        "illuminate/http": "~9",
        "illuminate/cache": "~9",
        "illuminate/console": "~9",
        "illuminate/queue": "~9",
        "illuminate/view": "~9",
        "illuminate/container": "~9",
        "illuminate/encryption": "~9",
        "illuminate/filesystem": "~9",
        "illuminate/collections": "~9",
        "xantios/mimey": "^2.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.6"
    },
    "suggest": {
        "a1comms/php-gds": "Provides legacy Datastore support"
    },
    "autoload": {
        "psr-4": {
            "AffordableMobiles\\GServerlessSupportLaravel\\": "src/AffordableMobiles/GServerlessSupportLaravel/"
        },
        "files": [
            "src/preload.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "AffordableMobiles\\GServerlessSupportLaravel\\GaeSupportServiceProvider",
                "AffordableMobiles\\GServerlessSupportLaravel\\Trace\\TraceServiceProvider"
            ]
        }
    }
}
