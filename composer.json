{
    "name": "eureciclo/gae-support-laravel",
    "description": "Google App Engine (Standard and Flexible Environment) runtime support for Laravel apps.",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Melrose",
            "email": "sam.melrose@a1comms.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "a1comms/eloquent-datastore": "~11",
        "a1comms/eloquent-sqlcommenter": "~11",
        "a1comms/opencensus": "~0",
        "a1comms/opencensus-exporter-stackdriver": "~0",
        "guzzlehttp/guzzle": "^7.4.1",
        "google/cloud": ">=0.175.0 <1.0.0",
        "firebase/php-jwt": "^6.3", 
        "illuminate/support": "~11",
        "illuminate/contracts": "~11",
        "illuminate/http": "~11",
        "illuminate/cache": "~11",
        "illuminate/console": "~11",
        "illuminate/queue": "~11",
        "illuminate/view": "~11",
        "illuminate/container": "~11",
        "illuminate/encryption": "~11",
        "illuminate/filesystem": "~11",
        "illuminate/collections": "~11",
        "symfony/console": "^7.0",
        "xantios/mimey": "^2.2"
    },
    "require-dev": {
        "phpmd/phpmd" : "@stable",
        "friendsofphp/php-cs-fixer": "^3.6"
    },
    "suggest": {
        "a1comms/php-gds": "Provides legacy Datastore support"
    },
    "autoload": {
        "psr-4": {
            "A1comms\\GaeSupportLaravel\\": "src/A1comms/GaeSupportLaravel/"
        },
        "files": [
            "src/preload.php"
        ]
    },
    "repositories": [
        {
          "type": "git",
          "url": "https://github.com/a1comms/opencensus-php-exporter-stackdriver.git"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "A1comms\\GaeSupportLaravel\\GaeSupportServiceProvider",
                "A1comms\\GaeSupportLaravel\\Trace\\TraceServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
